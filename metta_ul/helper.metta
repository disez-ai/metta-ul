(= (prop-value ($k $v)) $v)

(: props-values (-> Expression Expression))
(= (props-values $expr)
   (if (== $expr ()) ()
       (let* (($head (car-atom $expr))
              ($tail (cdr-atom $expr))
              ($head-new (prop-value $head))
              ($tail-new (props-values $tail))
             )
         (cons-atom $head-new $tail-new)
       )
   )
)

(= (df-values $id $expr) (let $vals (props-values $expr) (cons-atom $id $vals)))
